# Model
INSTRUCTOR_MODEL=your_model_name_here

## OpenAI (or OpenAI compatible)
INSTRUCTOR_OPENAI_API_KEY=your_openai_key_here
# Optional
# INSTRUCTOR_OPENAI_ORGANIZATION_ID=
# INSTRUCTOR_OPENAI_API_URL=
# INSTRUCTOR_OPENAI_API_PATH=

## Anthropic
ANTHROPIC_API_KEY=your_anthropic_key_here
ANTHROPIC_API_BASE_URL=https://api.anthropic.com  # Optional

## Local LLM (e.g., Ollama)
OLLAMA_API_BASE_URL=http://localhost:11434  # Default for local Ollama server

# General LLM Configuration
LLM_MODEL=gpt-3.5-turbo  # Model name depends on provider
LLM_MAX_RETRIES=3
LLM_TEMPERATURE=0.7

# Other Settings
PLAYWRIGHT_ENABLED=true  # Enable/disable Playwright for JS-heavy pages

# Search Providers
BRAVE_API_KEY=your_brave_api_key_here
SEARXNG_BASE_URL=http://localhost:8080
